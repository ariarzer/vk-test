{
  "name": "vk-test-task",
  "version": "0.1.0",
  "description": "Test task for VK",
  "main": "index.js",
  "scripts": {
    "test": "jest && npm run lint",
    "lint": "eslint \"**/*.js{,x}\"",
    "build": "webpack",
    "dev": "nodemon ./index.js localhost 8080",
    "dev-front": "webpack -w",
    "start": "npm run build && node index.js "
    "data-gen": "mkdir data; node scripts/generate.js > data/users.json",
  },
  "author": {
    "name": "Sofi Valitova",
    "email": "ariarzer@gmail.com"
  },
  "dependencies": {
    "dummy-json": "^2.0.0",
    "express": "^4.16.4",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "@download/blockies": "^1.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.5",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.3.0",
    "nodemon": "^1.18.6",
    "webpack": "^4.22.0",
    "webpack-cli": "^3.1.2"
  },
  "jest": {
    "transform": {}
  },
  "eslintConfig": {
    "extends": "airbnb",
    "env": {
      "browser": true,
      "node": true,
      "jest": true
    },
    "rules": {
      "import/extensions": "never",
      "jsx-a11y/label-has-for": false,
      "jsx-a11y/label-has-associated-control": false,
      "class-methods-use-this": 0
    },
    "parser": "babel-eslint"
  },
  "eslintIgnore": [
    "static/dist/"
  ]
}
